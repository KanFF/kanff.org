{
    "onepagemd-template-version": 0.2,
    "main-title": null,
    "title": "KanFF",
    "website-name": "kanff.org",
    "version": "vX.X",
    "author": "KanFF Team",
    "email": null,
    "link": "github.com/samuelroland/KanFF",
    "sourcelink": "",
    "link-placeholder": "GitHub",
