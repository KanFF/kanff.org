            "image-not-found-info": true,
            "image-not-found-path": "config/image-if-not-found.png",
            "relative-url-prefix": ""
        },
        "additionnal-values": "values.php"
    },
    "style": {
        "basic-style": "default_kanff",
        "font-list": "Open Sans, sans-serif",
        "font-size": "1em",
        "body": {
            "maxwidth": "1300px",
            "font-color": "#374151",
            "background-color": "#f7f7f7"
        },
        "load-css-files": [
            "css/md.css",
            ""
        ]
